{
  "version": "2",
  "plugins": [
    {
      "name": "fsharp",
      "wasm": {
        "url": "file://../../sqlc-gen-fsharp.wasm"
      }
    }
  ],
  "sql": [
    {
      "schema": "sqlite/schema.sql",
      "queries": "sqlite/query.sql",
      "engine": "sqlite",
      "codegen": [
        {
          "out": "sqlite",
          "plugin": "fsharp",
          "options": {
            "namespace": "Authors",
            "async": false
          }
        }
      ]
    }
  ]
}

