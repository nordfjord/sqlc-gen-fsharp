{
  "version": "2",
  "plugins": [
    {
      "name": "fsharp",
      "wasm": {
        "url": "file://../../sqlc-gen-fsharp.wasm"
      }
    }
  ],
  "sql": [
    {
      "engine": "sqlite",
      "schema": "schema.sql",
      "queries": "query.sql",
      "codegen": [
        {
          "out": "../src/sqlite",
          "plugin": "fsharp",
          "options": {
            "namespace": "SAuthors"
          }
        }
      ]
    }
  ]
}
